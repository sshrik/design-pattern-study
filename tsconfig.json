{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "abstract-factory-pattern/*": ["abstract-factory-pattern/*"],
      "factory-method-pattern/*": ["factory-method-pattern/*"],
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
